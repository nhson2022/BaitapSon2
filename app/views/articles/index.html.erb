<h2>Articles Listing</h2>
<div class="card">
	<div class="card-body">
		<h5 class="card-title">Home</h5>
		<% @articles.each do |article| %>
			<div class="container text-center">
				<div class="row align-items-start">
					<div class="col-2">
						<%= article.id %>
					</div>
					<div class="col-3">
						<%= article.title %>
					</div>
					<div class="col-7">
						<%= article.content %>
					</div>
				</div>
			</div>
		<% end %>
	</div>
</div>
