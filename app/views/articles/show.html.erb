<h2>Details Article</h2>
<div class="card">
	<div class="card-body">
		<h5 class="card-title">Home</h5>
		<% @articles.each do |article| %>
			<div class="container text-center">
				<div class="row align-items-start">
					<div class="col">
						<h4 class="text-primary"><%= article.title %></h4>
						<%= article.content %>
					</div>
				</div>
			</div>
		<% end %>
	</div>
</div>
